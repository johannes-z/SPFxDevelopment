{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "react-basics-wp-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/reactBasicsWp/ReactBasicsWpWebPart.js",
          "manifest": "./src/webparts/reactBasicsWp/ReactBasicsWpWebPart.manifest.json"
        }
      ]
    },
    "document-card-wp-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/documentCardWp/DocumentCardWpWebPart.js",
          "manifest": "./src/webparts/documentCardWp/DocumentCardWpWebPart.manifest.json"
        }
      ]
    },
    "tiny-mce-wp-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/tinyMceWp/TinyMceWpWebPart.js",
          "manifest": "./src/webparts/tinyMceWp/TinyMceWpWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "ReactBasicsWpWebPartStrings": "lib/webparts/reactBasicsWp/loc/{locale}.js",
    "DocumentCardWpWebPartStrings": "lib/webparts/documentCardWp/loc/{locale}.js",
    "TinyMceWpWebPartStrings": "lib/webparts/tinyMceWp/loc/{locale}.js"
  }
}
