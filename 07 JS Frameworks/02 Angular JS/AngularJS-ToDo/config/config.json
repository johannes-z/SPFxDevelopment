  "version": "2.0",
  "bundles": {
    "angular-js-to-do-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/angularJsToDo/AngularJsToDoWebPart.js",
          "manifest": "./src/webparts/angularJsToDo/AngularJsToDoWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
