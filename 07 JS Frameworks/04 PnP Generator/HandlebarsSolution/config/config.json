{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "handlebar-wp-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/handlebarWp/HandlebarWpWebPart.js",
          "manifest": "./src/webparts/handlebarWp/HandlebarWpWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "handlebars": "./node_modules/handlebars/dist/handlebars.amd.min.js"
  },
  "localizedResources": {
    "HandlebarWpWebPartStrings": "lib/webparts/handlebarWp/loc/{locale}.js",
    "PropertyControlStrings": "node_modules/@pnp/spfx-property-controls/lib/loc/{locale}.js"
  }
}